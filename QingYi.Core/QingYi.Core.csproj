<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1;net6.0;net7.0;net8.0;net9.0;net6.0-windows;net7.0-windows;net8.0-windows;net9.0-windows;net9.0-browser</TargetFrameworks>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>QingYi.Core</Title>
		<Version>5.2.0-pre6</Version>
		<Authors>QingYi-Studio</Authors>
		<Copyright>AGPL-3.0</Copyright>
		<PackageProjectUrl>https://github.com/QingYi-Studio/QingYi.Core</PackageProjectUrl>
		<RepositoryUrl>https://github.com/QingYi-Studio/QingYi.Core</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<Description>
			The core of our other projects.
			This project also has a lot of utility features, such as Base encoding, file information retrieval, file/folder compression, Shell, etc.
			Some annotations are generated by AI. If there are any errors, please provide feedback in time and we will correct them as soon as possible.
		</Description>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageIcon>icon.png</PackageIcon>
		<PackageTags>QingYi;Base;Base2;Base10;Base16;Base32;Base36;Base45;Base56;Base58;Base62;Base64;Encode;Decode;Codec;Shell;Folder;File;Compress;Regedit;Power</PackageTags>
		<NuGetAudit>False</NuGetAudit>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<ProduceReferenceAssembly>True</ProduceReferenceAssembly>
		<PackageReleaseNotes>
			- Support Deflate compression.
			- Support LZ77 compression.
		</PackageReleaseNotes>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<PackAsTool>False</PackAsTool>
		<SignAssembly>False</SignAssembly>
		<IncludeSymbols>True</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<AssemblyVersion>5.2.0.8</AssemblyVersion>
		<FileVersion>5.2.0.8</FileVersion>
	</PropertyGroup>

